{
    "version": 2,
    "builds": [
        {
            "src": "*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.ts",
            "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
        }
    ]
    
}
